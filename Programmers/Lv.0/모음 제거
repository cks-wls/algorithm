function solution(my_string) {
    let result = "";
    for (let i = 0; i < my_string.length ; i++){
        let value = my_string[i];
        if (value === 'a' || value === 'e' || value === 'i' || value === 'o' || value === 'u'){
            result += "";
        }else{
            result += value;
        }
    }
    return result;
}